{
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "indent": [
      2,
      4
    ],
    "no-underscore-dangle":"off",
    "react/jsx-indent": 4,
    "react/jsx-indent-props": 4,
    "react/no-did-mount-set-state": "off",
    "react/jsx-filename-extension": "off"
  }
}
